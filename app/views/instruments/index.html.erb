<div class="container">
  <h1>Available Instruments: <%= @search_term %></h1>
  <% @filtered_instruments.each do |instrument| %>
    <div class="container">
      <h2><%= instrument.model %></h2>
      <%#= TODO: link_to instrument.model, instrument_path(instrument) %>
      <p><%= instrument.location %></p>
      <p><%= instrument.description %></p>
      <p><%= instrument.price_per_day %> EUR/Day</p>
    </div>
  <% end %>
</div>
